        "from datetime import date\n",
        "sns.set_style(\"whitegrid\")\n",
        "plt.rcParams[\"figure.dpi\"] = 110\n",
        "\n",
        "START_DATE = \"2020-07-01\"\n",
        "TODAY      = date.today().strftime(\"%Y-%m-%d\")\n",
        "RF_RATE    = 0.02    # tasa libre usada en todo el notebook\n"
