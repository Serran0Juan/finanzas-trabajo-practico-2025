        "# 5 · ANÁLISIS TÉCNICO – acción principal (MELI)  ───────────────────────────\n",
        "import matplotlib.dates as mdates\n",
        "from ta.trend import adx\n",
        "\n",
        "TICKER = \"MELI\"                     # cambia si tu acción asignada es otra\n",
        "tech = add_technical_indicators(prices_df, TICKER)\n",
        "\n",
        "# --- ADX (fortaleza de tendencia) -----------------------------------------\n",
        "tech[\"ADX14\"] = adx(prices_df[TICKER], prices_df[TICKER], prices_df[TICKER], window=14)\n",
        "\n",
        "# --- 1) Precio + SMA50/200 + Bollinger ------------------------------------\n",
        "fig, ax = plt.subplots(figsize=(10,4))\n",
        "ax.plot(tech.index, tech[\"Close\"], label=\"Close\", lw=1)\n",
        "ax.plot(tech.index, tech[\"SMA50\"],  label=\"SMA 50\", lw=1)\n",
        "ax.plot(tech.index, tech[\"SMA200\"], label=\"SMA 200\", lw=1)\n",
        "ax.fill_between(tech.index, tech[\"BB_low\"], tech[\"BB_high\"],\n",
        "                color=\"grey\", alpha=0.15, label=\"Bollinger ±2σ\")\n",
        "ax.set_title(f\"{TICKER} – Precio, MAs y Bollinger\")\n",
        "ax.legend(); ax.yaxis.set_major_formatter(mdates.DateFormatter(\"%y\")); plt.show()\n",
        "\n",
        "# --- 2) MACD & RSI ---------------------------------------------------------\n",
        "fig, (ax1, ax2) = plt.subplots(2,1, figsize=(9,5), sharex=True)\n",
        "ax1.plot(tech.index, tech[\"MACD\"], label=\"MACD\"); ax1.plot(tech.index, tech[\"MACD_signal\"], label=\"Signal\")\n",
        "ax1.set_title(\"MACD\"); ax1.legend()\n",
        "ax2.plot(tech.index, tech[\"RSI14\"], label=\"RSI 14\"); ax2.axhline(70, ls=\"--\", c=\"red\"); ax2.axhline(30, ls=\"--\", c=\"green\")\n",
        "ax2.set_title(\"RSI\"); ax2.legend()\n",
        "plt.tight_layout(); plt.show()\n",
        "\n",
        "# --- 3) ADX ---------------------------------------------------------------\n",
        "tech[\"ADX14\"].plot(figsize=(9,2.5), title=\"ADX 14 (fortaleza de tendencia)\")\n",
        "plt.axhline(25, ls=\"--\", c=\"grey\"); plt.show()\n",
        "\n",
        "# --- 4) Back-test Golden/Death Cross SMA50/200 ----------------------------\n",
        "equity, sharpe_bt, dd_bt = backtest_ma_cross(tech)\n",
        "buyhold_ret = prices_df[TICKER].pct_change().dropna()\n",
        "buyhold_sh  = np.sqrt(252)*buyhold_ret.mean()/buyhold_ret.std()\n",
        "buyhold_dd, _, _ = max_drawdown(prices_df[TICKER])\n",
        "\n",
        "print(f\"MA-Cross  Sharpe {sharpe_bt:.2f}   Max DD {dd_bt:.2%}\")\n",
        "print(f\"Buy&Hold  Sharpe {buyhold_sh:.2f}   Max DD {buyhold_dd:.2%}\")\n",
        "\n",
        "equity.plot(figsize=(10,3), title=f\"{TICKER} – Equity curve estrategia MA-Cross\"); plt.show()\n"
      ],
