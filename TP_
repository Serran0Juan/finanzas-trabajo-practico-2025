      "cell_type": "code",
      "source": [
        "sim_df = simulate_portfolios(rets, n_port=3_000, rf=rf_rate)\n"
      ],
      "metadata": {
        "id": "aTHA4G-ZuXVP"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# ── Helper de emergencia: simulate_portfolios ───────────────────────────\n",
        "def simulate_portfolios(ret_df, n_port=3_000, rf=0.0,\n",
        "                        min_w=0.05, max_w=0.40, trans_cost=0.005, seed=0):\n",
        "    \"\"\"\n",
        "    Montecarlo sencillo para generar carteras aleatorias y métricas clave.\n",
        "    Retorna un DataFrame con columnas: Return, Vol, Sharpe + pesos por ticker.\n",
        "    \"\"\"\n",
        "    np.random.seed(seed)\n",
        "    mus   = ret_df.mean()*252\n",
        "    cov   = ret_df.cov()*252\n",
        "    names = list(ret_df.columns)\n",
        "    results = []\n",
        "\n",
        "    for _ in range(n_port):\n",
        "        w = np.random.uniform(min_w, max_w, len(names))\n",
        "        w /= w.sum()                           # normalizo al 100 %\n",
        "        port_ret = mus @ w - trans_cost\n",
        "        port_vol = np.sqrt(w.T @ cov @ w)\n",
        "        sharpe   = (port_ret - rf) / port_vol\n",
        "        results.append([port_ret, port_vol, sharpe, *w])\n",
        "\n",
        "    cols = [\"Return\", \"Vol\", \"Sharpe\"] + names\n",
        "    return pd.DataFrame(results, columns=cols)\n"
      ],
      "metadata": {
        "id": "ZG2X7j1Tu7CJ"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# 4 · OPTIMIZACIÓN DE CARTERA  (sin CVXPY – robusto)\n",
        "# --------------------------------------------------\n",
        "rf_rate = 0.02          # tasa libre\n",
        "\n",
        "# --- Aplana columnas si vienen como tuplas / MultiIndex ---------------\n",
        "if isinstance(prices_df.columns[0], tuple) or isinstance(prices_df.columns, pd.MultiIndex):\n",
        "    prices_df = prices_df.copy()\n",
        "    prices_df.columns = [c[-1] if isinstance(c, tuple) else c for c in prices_df.columns]\n",
        "\n",
        "# --- Rendimientos diarios --------------------------------------------\n",
        "rets = daily_returns(prices_df[TICKERS_MAIN])\n",
        "\n",
        "# 1) Simulación aleatoria (para dispersión)\n",
        "sim_df = simulate_portfolios(rets, n_port=3_000, rf=rf_rate)\n",
        "\n",
        "# 2) Frontera eficiente vía simulación (sin solver)\n",
        "ef, p_maxSharpe, p_minVol = efficient_frontier_sim(rets, rf=rf_rate)\n",
        "\n",
        "# 3) Punto intermedio (riesgo ≈ 12 %)\n",
        "target = ef.iloc[(ef[\"Vol\"] - 0.12).abs().idxmin()]\n",
        "\n",
        "# 4) Plot --------------------------------------------------------------\n",
        "fig, ax = plt.subplots(figsize=(8,5))\n",
        "ax.scatter(sim_df[\"Vol\"], sim_df[\"Return\"], c=sim_df[\"Sharpe\"],\n",
        "           cmap=\"viridis\", alpha=0.3, label=\"Carteras aleatorias\")\n",
        "ax.plot(ef[\"Vol\"], ef[\"Return\"], color=\"black\", lw=1.4, label=\"Frontera eficiente\")\n",
        "\n",
        "for lbl, pt, clr in [(\"Máx Sharpe\", p_maxSharpe, \"red\"),\n",
        "                     (\"Mín Vol\",    p_minVol,   \"blue\"),\n",
        "                     (\"Elegido\",    target,     \"orange\")]:\n",
        "    ax.scatter(pt[\"Vol\"], pt[\"Return\"], color=clr, s=80, zorder=5)\n",
        "    ax.annotate(lbl, (pt[\"Vol\"], pt[\"Return\"]), xytext=(5,5),\n",
        "                textcoords=\"offset points\", color=clr)\n",
        "\n",
        "ax.set_xlabel(\"Volatilidad anualizada\")\n",
        "ax.set_ylabel(\"Retorno anual esperado\")\n",
        "ax.set_title(\"Frontera eficiente (restricciones 5 % – 40 %)\")\n",
        "ax.legend(); plt.show()\n",
        "\n",
        "print(\"Máx Sharpe\\n\", p_maxSharpe.round(4))\n",
        "print(\"\\nMín Volatilidad\\n\", p_minVol.round(4))\n",
        "print(\"\\nPunto elegido (~12 % riesgo)\\n\", target.round(4))\n"
      ],
