    {
      "cell_type": "code",
      "source": [
        "# ── Completar PEG e Interest Cover ────────────────────────────────────────\n",
        "def calc_ratios(tk):\n",
        "    info   = yf.Ticker(tk).info\n",
        "    pe     = info.get(\"trailingPE\")\n",
        "    growth = info.get(\"earningsQuarterlyGrowth\")        # 0.25 → 25 %\n",
        "    peg    = pe / (growth*100) if pe and growth else None\n",
        "\n",
        "    ebitda = info.get(\"ebitda\")\n",
        "    int_exp = abs(info.get(\"interestExpense\", 0)) or None\n",
        "    icov   = ebitda / int_exp if ebitda and int_exp else None\n",
        "    return peg, icov\n",
        "\n",
        "for tk in ratios.index:\n",
        "    peg, icov = calc_ratios(tk)\n",
        "    ratios.loc[tk, \"PEG\"]            = round(peg, 2)  if peg  else \"–\"\n",
        "    ratios.loc[tk, \"Interest Cover\"] = round(icov, 1) if icov else \"–\"\n",
        "\n",
        "display(ratios.style.format(precision=2, na_rep=\"–\"))\n"
      ],
