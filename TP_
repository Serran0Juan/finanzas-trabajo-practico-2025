      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "from IPython.display import display\n",
        "\n",
        "# --- 1) precios normalizados -------------------------------------------------\n",
        "(prices_df[TICKERS_MAIN] / prices_df[TICKERS_MAIN].iloc[0] * 100).plot(\n",
        "    figsize=(10,4), title=\"Precios normalizados (Base 100)\")\n",
        "plt.ylabel(\"Índice 100 = 01-jul-2020\")\n",
        "plt.show()\n",
        "\n",
        "# --- 2) retornos diarios -----------------------------------------------------\n",
        "rets = daily_returns(prices_df[TICKERS_MAIN])\n",
        "\n",
        "# --- 3) métricas de riesgo ---------------------------------------------------\n",
        "rows = []\n",
        "for t in TICKERS_MAIN:\n",
        "    var95, cvar95 = var_cvar(rets[t])\n",
        "    dd, start, end = max_drawdown(prices_df[t])\n",
        "    jb_stat, jb_p = jarque_bera(rets[t])\n",
        "    rows.append([t, var95, cvar95, dd, jb_stat, jb_p])\n",
        "\n",
        "risk_df = pd.DataFrame(rows, columns=[\n",
        "    \"Ticker\", \"VaR 95%\", \"CVaR 95%\", \"Max Draw-down\", \"JB stat\", \"JB p-value\"\n",
        "]).set_index(\"Ticker\")\n",
        "display(risk_df.style.format({\n",
        "    \"VaR 95%\":\"{:.2%}\", \"CVaR 95%\":\"{:.2%}\", \"Max Draw-down\":\"{:.2%}\",\n",
        "    \"JB stat\":\"{:.1f}\", \"JB p-value\":\"{:.3f}\"\n",
        "}))\n"
      ],
